{
  "name": "scss-module-library",
  "version": "1.0.0",
  "description": "My favorite UI library!",
  "keywords": [
    "ui",
    "modules",
    "components"
  ],
  "main": "index.html",
  "scripts": {
    "lint": "eslint src/**.js",
    "js-bundle": "cat node_modules/jquery/dist/jquery.min.js src/vendor/*.js src/modules/*.js > demo/bundle.js",
    "build-js": "npm run js-bundle",
    "watch-js": "watch-run -p 'src/**' & npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorgepinon/scss-module-library.git"
  },
