  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jorgepinon/scss-module-library/issues"
  },
  "homepage": "https://github.com/jorgepinon/scss-module-library#readme",
  "devDependencies": {
    "eslint": "^3.2.2",
    "browserify": "latest",
    "uglify-js": "~2.4",
    "watch-run": "~1.2"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "responsive-nav": "^1.0.39"
  }
}
